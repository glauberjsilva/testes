package com.example.project;

import com.exemple.exceptions.CalculatorException;

public class Calculator {

	public int divide(int a, int b) throws CalculatorException {
		if(b == 0)
			throw new CalculatorException("NÃ£o pode dividir por zero");
		
		return a + b;
	}

}
